		_gaq.push(['_trackSocial', 'facebook', 'comment']);
	});
	FB.Event.subscribe('comment.remove', function(targetUrl) {
		_gaq.push(['_trackSocial', 'facebook', 'uncomment']);
	});  
    }
  } catch (e) {}
};


/**
