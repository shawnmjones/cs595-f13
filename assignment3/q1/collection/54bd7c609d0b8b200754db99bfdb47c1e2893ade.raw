<script type="text/javascript" maxAge=60>  
var timeout	= 10;
var closetimer	= 0;
var ddmenuitem	= 0;
var bcplayer = 0;
var	bcplayer = document.getElementById("featuredvideos");
	if (bcplayer) bcplayer.setAttribute("style","margin-top:-100000px; display:none; visibility:hidden;");

	// open hidden layer
	function mopen(id)
	{	
	// cancel close timer
	mcancelclosetime();

	// close old layer
	if(ddmenuitem) ddmenuitem.setAttribute("style", "margin-top:-10000px; display:none; visibility:hidden;");	

	// get new layer and show it
	ddmenuitem = document.getElementById(id);
	ddmenuitem.style.visibility = 'visible';
	ddmenuitem.setAttribute("style", "margin-top:-40px; display:block; visibility:visible;");	
	//If SMmenu -- move brightcove player div to normal viewport.
	if (id === "SMmenu") {
		document.getElementById('youtube').setAttribute("style","margin-top:0px;display:block;");
	
	}


}
// close showed layer
function mclose()
{
	if(ddmenuitem) ddmenuitem.style.visibility = 'hidden';
	if(ddmenuitem) ddmenuitem.setAttribute("style", "margin-top:-10000px;display:none;visibility:hidden;");	
		// As no idea is passed, any time "mclose" is called, move brightcove player to -10000 above screen.
		document.getElementById('youtube').setAttribute("style","margin-top:-10000px;display:none;");
	
}

// go close timer
function mclosetime()
{
	closetimer = window.setTimeout(mclose, timeout);
}

// cancel close timer
function mcancelclosetime()
{
	if(closetimer)
	{
		window.clearTimeout(closetimer);
		closetimer = null;
	}
}

// close layer when click-out
document.onclick = mclose; 
</script>

