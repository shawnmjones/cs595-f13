	    if (document.location.pathname.match(/^\/story/)) {
			  DailyKos.Metrics.track('Viewed Story', {  url: "http://www.dailykos.com/story/2013/03/17/1194715/-Midday-open-thread", 
			                                            blog: "main",
			                                            author: $('.author').children('a').first().html(),
