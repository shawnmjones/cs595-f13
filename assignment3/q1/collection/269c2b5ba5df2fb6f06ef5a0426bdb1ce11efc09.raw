function parseDate(date) {
		var date = date.split(" ");
		var newDate = "";
		
		var i = 0;
		while(i < 3) {	
			if(i > 0)
				newDate += " ";
			
			newDate += date[i];
		
			i++;
		}
		
		return newDate;
	}

$(document).ready(function() {
		
	var rssUrl = "http://blogs.technet.com/b/next/rss.aspx?Tags=Featured"; // set the rss url here
	var isHome = true;
		
	$.ajax({
        type: "GET",
		url: rssUrl, 
		dataType: "xml",
		success: function(xml) {
			var i = 0;
			var post = "<div>";
		
			$(xml).find('item').each(function(){
				
				var numItems = 3;
				var postClass = "post";

				
				if(!isHome)
				{
					postClass = "subpost";
					numItems = 3;
				}
				
				if(i == numItems)
				{
					post += "</div>";
					$(".scrollable-sm .items-sm").append(post);
					
					i = 0;
					post = "<div>";
				}
				
				post += '<div class="' + postClass + '">' + '<p class="date">' + parseDate($(this).find('pubDate').text()) + '</p>' + '<h4><a href="' + $(this).find('link').text() + '">' + $(this).find('title').text() + '</a></h4>' + $(this).find('description').text() + '</div>';

				
				i++;
			});
			
			post += "</div>";
			$(".scrollable-sm .items-sm").append(post);
		}
	});
	
	// initialize scrollable
	// $(".scrollable-sm").scrollable-sm();
	// this goes in the document.ready at the bottom, but before the last });
	
	// comment out this scrollable stuff --
	//                         			  |
	//                                    V
	
	// initialize scrollable-sm
	//$(".scrollable-sm").scrollable-sm();

	// tim's handcrafted scrollable widget of goodness
	var leftPos = 0;
	var currentPage = 0;
		
	$(".right").click(function() {
		if(currentPage < ($(".items-sm").children().size() - 1))
		{
			leftPos -= 840;
			$(".items-sm").animate({left:leftPos});
			currentPage++;
		}
	});
		
	$(".left").click(function() {
		if(currentPage > 0)
		{
			leftPos += 840;
			$(".items-sm").animate({left:leftPos});
			currentPage--;
		}
	});

});

</script>

