                                    break;
                                case 'news':
                                    src_desc = 1186;
                                    break;
                                case 'restaurants':
                                case 'dining':
                                case 'food':
                                    src_desc = 1187;
                                    break;
                                                            }
                        }
                        return '8|53|'+src_desc;
                    }
