	function changeMap(columnID) {
		currentLayers[columnID][0].setMap(null);
		if (columnID == 0) {
			var requestedLayer = stateLayersA[currentCandidates[columnID][0]];
		} else if (columnID == 1) {
			var requestedLayer = stateLayersB[currentCandidates[columnID][0]];
		}
		
		//reset zoom and center 
		mapsArray[columnID].setZoom(stateZoomLevel); 
		mapsArray[columnID].setCenter(stateLatLng);
		
		//attach the requested layer
		currentLayers.splice(columnID, 1, [requestedLayer, 'us']);
		currentLayers[columnID][0].setMap(mapsArray[columnID]); 
		
		//rebuilds the correct key
		buildKeys(columnID); 
		
		//change the selection of the United States/Wisconsin buttons back to United States
		var mapForms = document.getElementById('mapForm'+columnID).getElementsByTagName('option')[0];
		mapForms.selected = 'selected';     
		
	}
	
	function changeLevel(mapLevel, columnID) { //passing requested map layer, not the layer you're currently on. 
		
		//toggle the key values
		for (var p=0; p < stateRange.length; p++) {
				if (columnID == '0') {
					console.log(columnID = '0');
					var keyTextA = document.getElementById("keyTextA"+p);
					if (mapLevel == "us") {
						keyTextA.innerHTML = stateRange[p];
					} else if (mapLevel == "wis") {
						keyTextA.innerHTML = zipRange[p];
					}
				} else if (columnID == '1') {
					console.log("columnString = 1");
					var keyTextB = document.getElementById("keyTextB"+p);
					if (mapLevel == "us") {
						keyTextB.innerHTML = stateRange[p];
					} else if (mapLevel == "wis") {
						keyTextB.innerHTML = zipRange[p];
					}
				}//column if/else
		}//for loop

		//access the correct map to switch
		var mapCall = mapsArray[columnID];
		
		//toggle the map layer
		if (mapLevel == 'us') {
			if (columnID == 0) {
				//remove layer
				wisLayersA[currentCandidates[columnID][0]].setMap(null);
				
				//reset zoom level and center point
				mapsArray[columnID].setZoom(stateZoomLevel); 
				mapsArray[columnID].setCenter(stateLatLng);
				
				//add requested layer
				var requestedLayer = stateLayersA[currentCandidates[columnID][0]]; 
				requestedLayer.setMap(mapCall);
				
				//replace currentLayers array
				currentLayers.splice(columnID, 1, [requestedLayer, 'us']); 
			} else if (columnID == 1){
				//remove layer
				wisLayersB[currentCandidates[columnID][0]].setMap(null);
				
				//reset zoom level and center point
				mapsArray[columnID].setZoom(stateZoomLevel); 
				mapsArray[columnID].setCenter(stateLatLng);
				
				//add requested layer
				var requestedLayer = stateLayersB[currentCandidates[columnID][0]]; 
				requestedLayer.setMap(mapCall);
				
				//replace currentLayers array
				currentLayers.splice(columnID, 1, [requestedLayer, 'us']);
			}
			
		} else if (mapLevel == 'wis') {
				if (columnID == 0) {
					//remove layer
					stateLayersA[currentCandidates[columnID][0]].setMap(null);

					//reset zoom level and center point
					mapsArray[columnID].setZoom(wisZoomLevel); 
					mapsArray[columnID].setCenter(wisLatLng);

					//add requested layer
					var requestedLayer = wisLayersA[currentCandidates[columnID][0]]; 
					requestedLayer.setMap(mapCall);

					//replace currentLayers array
					currentLayers.splice(columnID, 1, [requestedLayer, 'wis']); 
				} else if (columnID == 1){
					//remove layer
					stateLayersB[currentCandidates[columnID][0]].setMap(null);

					//reset zoom level and center point
					mapsArray[columnID].setZoom(wisZoomLevel); 
					mapsArray[columnID].setCenter(wisLatLng);

					//add requested layer
					var requestedLayer = wisLayersB[currentCandidates[columnID][0]]; 
					requestedLayer.setMap(mapCall);

					//replace currentLayers array
					currentLayers.splice(columnID, 1, [requestedLayer, 'wis']);
				}
		}
		

	}
	
	
