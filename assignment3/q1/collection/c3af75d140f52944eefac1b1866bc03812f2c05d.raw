<script>
	
/// LOADING XML -- Clickability only. This can't be use to test locally - permissions issues.
	if (window.XMLHttpRequest) { // code for IE7+, Firefox, Chrome, Opera, Safari
	  xmlhttp=new XMLHttpRequest();
	} else {// code for IE6, IE5
	  xmlhttp=new ActiveXObject("Microsoft.XMLHTTP");
	}

	var envHost = "";
		if(window.location.host.indexOf("dev.www.") > -1) { envHost = "dev."; }	
		var xmlurl = "http://" + envHost + "www.jsonline.com/includes/contributions_july_update.xml"; 

	xmlhttp.open("GET",xmlurl,false);
	xmlhttp.send();
	xmlDoc=xmlhttp.responseXML; 

// Load the Visualization API
// This is for the google charts -- has to run up here or throws an error.
	google.load('visualization', '1.0', {'packages':['corechart']});

// Don't know if we want this since the buildCharts function should be called from somewhere else.
// // Set a callback to run when the Google Visualization API is loaded.
// google.setOnLoadCallback(buildCharts);

//global variables
var forms = document.getElementById("mainForms").getElementsByTagName("form"); //array of profile forms 
var options; 
var xmlCandidates = xmlDoc.getElementsByTagName("candidate"); 
var currentCandidates = [];
var currentLayers = []; 
var nameRemindersA = []; 
var barRemindersA = [];
var nameRemindersB = []; 
var barRemindersB = [];
var map; 
var mapsArray = []; 
var stateLayersA = [];
var wisLayersA = []; 

//back ups for when a user accidently tries to compare the same candidate
var stateLayersB = [];
var wisLayersB = [];

var stateLatLng = new google.maps.LatLng(38.498091, -98.320232);
var wisLatLng = new google.maps.LatLng(44.82744, -89.659069);
var stateFusionID; 
var wisFusionID; 
var stateZoomLevel = 3; 
var wisZoomLevel = 6;
var stateRange = ["$1", "10,000", "100,000", "250,000+"]; //map keys - national level
var zipRange = ["$1", "1,000", "5,000", "10,000+"]; //map keys - wisconsin level
var repColor = ["#ead0cd", "#d6a09b", "#c17168", "#ac4136"]; 
var demColor = ["#d7dde7", "#afbbcf", "#8699b6", "#5e779e"];

window.onload = function() {
	initialize();
} //window on load


//////////////////////////////// THESE FUNCTIONS SET UP ALL OF THE DEFAULTS ////////////////////////////////
function initialize() {
	//need to check selection in each form. this loop will run twice (once for each form)
	for (var j=0; j < 3; j++) {
		var nameReminderA = document.getElementById("nameRemindA"+j); 
		nameRemindersA.push(nameReminderA); 
		
		var barReminderA = document.getElementById("barRemindA"+j); 
		barRemindersA.push(barReminderA);  
		
		var nameReminderB = document.getElementById("nameRemindB"+j); 
		nameRemindersB.push(nameReminderB); 
		
		var barReminderB = document.getElementById("barRemindB"+j); 
		barRemindersB.push(barReminderB);
	}
	
	for (var j=0; j < forms.length; j++) {
		options = forms[j].getElementsByTagName("option");
		
		for (var i=0; i < options.length; i++) {
			if(options[i].selected) {
				
				var sCandID = i; 
				var sColID = j; 
				var sPartyID = xmlDoc.getElementsByTagName("candidate")[i].getAttribute("partyID"); 
				var sChartID = "chart"+j; //placeholder div IDs
				var sTimelineID = "chart2"; 
				var sArrangeID = 0; //defaults the charts to type of donor -- in the end should be in-state v. out of state
				
				currentCandidates.push([sCandID, sPartyID]); //places the selected Candidates in an array... this should only be replaced when someone clicks on the top two forms.
				
				//call build functions and send appropriate ID numbers.
				buildProfile(sColID);
				buildKeys(sColID); 
				makeMaps(sColID); 
				loadMaps('us', sColID); 
				buildDonors(sColID);
				addNameReminders(sColID); 
				
				if (sArrangeID == 0 || sArrangeID == 1) {
					buildCharts(sCandID, sColID, sChartID, sArrangeID);
				} else if (sArrangeID == 2) {
					 
				}
				
			} else {
				
			}//selected if/else
			
			 
			
		} //options loop
			
		} //forms loop

} //intialize function
	
//Populate the profile section
	
	function buildProfile(columnID) {
		
		//Apply correct color to color bar
		var bar = document.getElementById("bar"+columnID); //bar element in the HTML
		var barColor = xmlCandidates[currentCandidates[columnID][0]].getAttribute("color"); //color from xml document
		bar.style.backgroundColor = barColor;  
		
		//Load candidate name
		var name = document.getElementById("name"+columnID); 
		name.innerHTML = xmlCandidates[currentCandidates[columnID][0]].getAttribute("label");
		
		//Load total funds raised
		var totalP = document.getElementById("total"+columnID); //total funds raised paragraph tag
		var totalNum = xmlCandidates[currentCandidates[columnID][0]].getAttribute("total"); //total funds raised from xml document
		totalP.innerHTML = "$"+totalNum; 
		
		//Load avg. size of contributions
		var avgP = document.getElementById("avg"+columnID); //total funds raised paragraph tag
		var avgNum = xmlCandidates[currentCandidates[columnID][0]].getAttribute("average"); //total funds raised from xml document
		avgP.innerHTML = "$"+avgNum;
		
	}
	
	
	function addNameReminders(columnID) {
		
		var currentName = xmlCandidates[currentCandidates[columnID][0]].getAttribute('label'); 
		var currentColor =  xmlCandidates[currentCandidates[columnID][0]].getAttribute('color');
		
		for (var i=0; i < 3; i++) {
			if (columnID == '0') {
				var rNameA = nameRemindersA[i]; 
				var rBarA = barRemindersA[i];
				rNameA.innerHTML = currentName; 
				rBarA.style.backgroundColor = currentColor;
			} else if (columnID == '1') {
				var rNameB = nameRemindersB[i]; 
				var rBarB = barRemindersB[i];
				rNameB.innerHTML = currentName; 
				rBarB.style.backgroundColor = currentColor;
			}
			
		}
	}
	
	function buildKeys(columnID) {
		
		var keyToRemove = document.getElementById("key"+columnID);
			if (keyToRemove.hasChildNodes()) {
		    	while ( keyToRemove.childNodes.length >= 1 ) {
		        	keyToRemove.removeChild( keyToRemove.firstChild );       
		    } 
		}
		
		var key = document.getElementById("key"+columnID);
		var labels = document.createElement('div');
		labels.setAttribute('class', 'labels');
		labels.setAttribute('className', 'labels'); 
		var boxes = document.createElement('div');
		boxes.setAttribute('class', 'boxes');
		boxes.setAttribute('className', 'boxes');
		key.appendChild(labels);
		key.appendChild(boxes);
		
		for (var m=0; m < repColor.length; m++) {
			var keyText = document.createElement('p'); 
			keyText.setAttribute('class', 'keyText');
			keyText.setAttribute('className', 'keyText');
			if (columnID == 0) {
				keyText.setAttribute('id', 'keyTextA'+m); 
			} else if (columnID == 1) {
				keyText.setAttribute('id', 'keyTextB'+m);
			}
			keyText.innerHTML = stateRange[m]; 
			
			labels.appendChild(keyText);
	
			var keyBox = document.createElement('div'); 
			keyBox.setAttribute('class', 'keyBox');
			keyBox.setAttribute('className', 'keyBox'); 
			keyBox.setAttribute('id', 'keyBox'+m);
			keyBox.innerHTML = " "; 
				
			if (currentCandidates[columnID][1] == 0) {
				keyBox.setAttribute('style', 'background-color:'+repColor[m]); 
			} else if (currentCandidates[columnID][1] == 1) {
				keyBox.setAttribute('style', 'background-color:'+demColor[m]);
			}
				
			boxes.appendChild(keyBox);
		}
	}
	

//make maps, layers and put into arrays, don't add any layers to the map yet
	function makeMaps(columnID) {
		//makeMaps
		map = new google.maps.Map(document.getElementById('map'+columnID), {
		  center: stateLatLng,
		  zoom: stateZoomLevel,
		  mapTypeId: 'roadmap'
		});
		
		mapsArray.push(map); 
		
			//make layers 
			for (var i=0; i < options.length; i++) { //options defined in the initalization function
				stateFusionID = xmlCandidates[i].getAttribute("stateFusion");
				wisFusionID = xmlCandidates[i].getAttribute("wisFusion");
			
				//default = state layer
				var stateLayer = new google.maps.FusionTablesLayer({
			  		query: {
			    		select: 'geometry',
			    		from: stateFusionID
			  		},
				});
				stateLayersA.push(stateLayer); 

				//make the wisconsin layer but don't place it on the map yet
				var wisLayer = new google.maps.FusionTablesLayer({
			  		query: {
			    		select: 'geometry',
			    		from: wisFusionID
			  		},
				});
				wisLayersA.push(wisLayer);
			}
			
			//make layers 
			for (var i=0; i < options.length; i++) { //options defined in the initalization function
				stateFusionID = xmlCandidates[i].getAttribute("stateFusion");
				wisFusionID = xmlCandidates[i].getAttribute("wisFusion");
			
				//default = state layer
				var stateLayerB = new google.maps.FusionTablesLayer({
