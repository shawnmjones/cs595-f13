    return false;
  }
 
  function cdFocusNonReplyPostTitle() {
    document.getElementById('cdNonReplyPostBoxBodyInput').focus();
    return false;
  }

  function cdCloseNonReplyPostBox() {
    document.getElementById('cdNonReplyPostBoxContent').style.display = 'none';
    document.getElementById('cdNonReplyPostBoxButton').style.display = 'block';
    document.getElementById('cdNonReplyPostBoxForm').reset();
    cdDisableNonReplyPostForm = 0;
    return false;
  }
                                                                                                                  
  function cdNonReplyPostCancelKey(e) {
    var code;
    if (e.keyCode) code = e.keyCode;
    else if (e.which) code = e.which;
    if (code == 32) {
      cdCloseNonReplyPostBox();
      return false;
    }
  }

  function cdIsPostValid(form) {
    var body = '' + form.bodyText.value;

    var whitespace = new RegExp("^" + String.fromCharCode(0x5c) + "s*$");

    if ( (body == '') || (whitespace.test(body)) ) {
      alert("The discussion body is empty. Please add text to it.");
      return false;
    }

    var maxLen = 16000;
    if ( body.length > maxLen ) {
      alert( cdGetLocalizedString('err_thread_post_body_n_too_long', {'length':(body.length - maxLen)}) );
      return false;
    }
    if ( cdDisableNonReplyPostForm++ ) {
      return false;
    }

    return true;
  }

  function cdCheckNonReplyPost(form) {
    var isOfficialComment  = form.officialComment.checked;
    var hasCamsName = credentials['hasCamsName'];
    var custRole = credentials['role'];
    var camsName = credentials['camsName'];
    var badgeHTML = credentials['badgeHTML'];
    var hasAgreedToTerms = credentials['hasAgreedToTerms'];
    var hasAuthorizedBadge = credentials['hasAuthorizedBadge'];

    if (isOfficialComment) {
      if (!hasAgreedToTerms && !hasAuthorizedBadge) {
        cdOCTermsPopover.showTermsPopover(cdOCTermsPopoverCallback, null);
        return false;
      }

      if (!hasCamsName) {
        cdPenNamePopover.showPenNamePopover(cdPenNamePopoverPostCallback, null, badgeHTML, null,  0); 
        return false;
      }
    } 

    return cdIsPostValid(form);
  }


