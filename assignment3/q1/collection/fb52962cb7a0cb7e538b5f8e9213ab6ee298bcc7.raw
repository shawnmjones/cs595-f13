<script>
//
// Create the comment editor
//
function createCommentEditor() {
    var tbButtons = [
                  ["bold", "italic", "underline", "strikethrough","separator",
                  "justifyleft", "justifycenter", "justifyright", "justifyfull", "separator",
                  "orderedlist", "unorderedlist", "separator",
                  "undo", "redo", "removeformat", "separator"]
    ];

    var instrTxt = "Your comments will go here. Use this space to tell others what you think of the product. " + 
                   "What do you like about it? What don't you like? Why did you choose to add it to your list?";

    var taOptions =  {
                      instructionText:   instrTxt,
                      defaultText:       '',
                      width:             asinpop_COMMENT_TEXTAREA_WIDTH+'px',
                      height:            asinpop_COMMENT_TEXTAREA_HEIGHT+'px',
                      toolbarButtons:    tbButtons,
                      enablePreview:     false,
                      showEditButton:    false,
                      showDoneButton:    false,
                      initialState:      null,
                      previewCallback:   null,
                      editCallback:      null
                     };

    var comment = addWysiwygTextarea('asinText', taOptions);

    return comment;
}

function createTextarea() {
    var width  = asinpop_COMMENT_TEXTAREA_WIDTH  + 'px';
    var height = asinpop_COMMENT_TEXTAREA_HEIGHT + 'px';
    return '<textarea '
         + 'onfocus="commentAreaOnFocus(this);"'
         + 'onblur="commentAreaOnBlur(this);"'
         + 'class="asinpop-Comment-Default"'
         + 'id="asinTextarea" style="width: ' + width + ' ; height: ' + height + ' ;">'
         + ''
         + '</textarea>';
}

//
// Creates a save button
//
function createSaveButton() {
	return '<a class="asinpop-Comment-SaveButtonLink" href="javascript:saveCommentAction();">'
		 + buttonDoneLargeTag
         + '</a>';

}

//
// Creates a save button
//
function createAddAnotherButton() {
	return '<a class="asinpop-Comment-SaveButtonLink" href="javascript:addAnotherAction();">'
		 + buttonAddAnotherTag
         + '</a>';

}

//
// This function is called to populate the comment area of the popover just
// before it is shown.
//
function populateCreatePane() {
	var content = '<table border="0" cellpadding="0" cellspacing="0">'
				+ '<tr>'
				+ '  <td>'
				+ '    <div class="asinpop-Comment-Header">'
				+ '      <span class="asinpop-Comment-HeaderText">Add your comments here: </span>' 
				+ '      '
				+ '    </div>'
				+ '  </td>'
				+ '<tr>'
				+ '</tr>'
				+ '  <td>'
				+ '    <div class="asinpop-Comment-Editor">'
				+        createCommentEditor()
				+ '    </div>'
				+ '  </td>'
				+ '<tr>'
				+ '</tr>'
				+ '  <td align="right">'
				+ '    <table border="0" cellpadding="0" cellspacing="0">'
				+ '      <tr>'
				+ '        <td>'
				+ '          <div class="asinpop-Comment-SaveButton">'
				+ 		        createAddAnotherButton()
				+ '          </div>'
				+ '        </td>'
				+ '        <td>'
				+ '          <div class="asinpop-Comment-SaveButton">'
				+ 		       createSaveButton()
				+ '          </div>'
				+ '        </td>'
				+ '      </tr>'
				+ '    </table>'
				+ '  </td>'
				+ '</tr>'
				;

	return content;
}

function commentAreaOnFocus(txt) {
  if (txt.value == commentPrompt) {
    txt.value = '';
    txt.className = '';
  }
}

function commentAreaOnBlur(txt) {
  if (txt.value == '') {
    txt.value = commentPrompt;
    txt.className = 'asinpop-Comment-Default';
  }
}
</script>

