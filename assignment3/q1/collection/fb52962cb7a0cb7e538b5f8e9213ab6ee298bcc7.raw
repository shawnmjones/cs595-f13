  });

  jQuery("#cdRetryGetOfficialCommentInfoCallLink").click(function() {
    cdMakeGetOfficialCommentInfoCall(true);
    return false;
  });


  jQuery("#cdOpenNonReplyPostBoxLink").click(function() {
