  credentials['camsName'] = '';
  credentials['isAuthorizedCall'] = 0;
  credentials['hasAgreedToTerms'] = 0;

  // These sections are mutually exclusive
  var officialContentSections = {
    "allowed"        : "#cdAuthorizedContent",
    "error"          : "#cdAuthorizationError",
    "not-allowed"    : "#cdUnauthorizedContent",
    "already-posted" : "#cdAlreadyPostedOC",
    "max-oc"         : "#cdMaxOC"
  }

  function cdShowSection(section) {  
      jQuery("#cdOfficialCommentContent").show();
      for (var contentSection in officialContentSections) {
        if (contentSection == section) {
          jQuery(officialContentSections[contentSection]).show();
        } else {
          jQuery(officialContentSections[contentSection]).hide();
        }
      }
  }

  function cdMakeGetOfficialCommentInfoCall(forceCall) {

    return;

    if (credentials['isAuthorizedCall'] && !forceCall) {
      // Already made call and no force/retry
      return;
    }

    credentials['isAuthorizedCall'] = 1;

    var forumTypeId = '';
    var threadAnchorTypeId = '';
    var threadAnchorInstanceId = '';

    jQuery("#cdNonReplyPostBoxForm input:hidden").each(
        function() {
          var name = jQuery(this).attr("name");
          if (name == "forumTypeName") {
            forumTypeId = jQuery(this).val();
          } else if (name == "threadAnchorTypeID") {
            threadAnchorTypeId = jQuery(this).val();
          } else if (name == "threadAnchorInstanceID") {
            threadAnchorInstanceId = jQuery(this).val();
          }
        }
    );

    var params = {
      sessionId:              '191-3668107-7574846',
      forumTypeId:            forumTypeId,
      threadAnchorTypeId:     threadAnchorTypeId,
      threadAnchorInstanceId: threadAnchorInstanceId,
      requestID: '13Q8DC0MWMTBE25QW378'
    };

    var getOfficialCommentInfoOptions = {
      type:     'POST',
      dataType: 'json',
      url:      '/gp/forum/cd/du/getOfficialCommentInfo.html',
      success:  cdHandleGetOfficialCommentInfo,
      error:    cdHandleGetOfficialCommentInfoError,
      data:     params
    };

    jQuery.ajax(getOfficialCommentInfoOptions);
  }

  function cdHandleGetOfficialCommentInfo(data) {
    var isOfficialCommentAllowed  = data['isOfficialCommentAllowed'];
    var role                      = data['role'];
    var badgedDisplayName         = data['badgedDisplayName'];
    var badgeHTML                 = data['badgeDisplay'];
    var token                     = data['token'];
    var officialSecureToken       = data['cdSecureToken'];
    var rejectionCode             = data['rejectionCode'];

    if (isOfficialCommentAllowed) {
      // show officialCommentAuthorizedContent
      if (badgedDisplayName == '') {
        jQuery("#cdBadgedPreview").hide();
        jQuery("#cdNoPennameMsg").show();
      } else {
        jQuery("#cdBadgedPreview").show();
        jQuery("#cdBadgedDisplay").html(badgedDisplayName);
      }
      jQuery("#officialRole").val(role);
      jQuery("#officialToken").val(token);
      jQuery("#officialSecureToken").val(officialSecureToken);

      credentials['role'] = role;
      credentials['badgeHTML'] = badgeHTML;

      cdShowSection("allowed");
    } else {
      if (rejectionCode == 'ALREADY_POSTED') {
        cdShowSection("already-posted");
      } else if (credentials['hasAuthorizedBadge']) {
        if (rejectionCode == 'MAX_OC') {
          cdShowSection("max-oc");    
        } else if (rejectionCode == 'NOT_OFFICIAL') {
          cdShowSection("not-allowed");
        }
      }
    }
    
    return;
  }

  function cdHandleGetOfficialCommentInfoError() {
    if (credentials['hasAuthorizedBadge']) {
      cdShowSection("error");
    }
  }

  function cdRedoPost() {
    // JQuery submit does not trigger onsubmit events
    // must do it manually
    var form = jQuery("#cdNonReplyPostBoxForm");
    var valid = cdCheckNonReplyPost(form[0]);
    if (valid) {
      form.submit();
