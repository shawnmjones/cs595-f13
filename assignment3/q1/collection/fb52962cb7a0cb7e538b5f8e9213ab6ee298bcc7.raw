  });

  jQuery(document).bind('cdTrackingStateChanged', function(e, isSubscribed) {
    if (isSubscribed) {
      jQuery('#cdNonReplyAlreadyTracking').show();
      jQuery('#cdNonReplyTrack').hide();
    }
    else {
      jQuery('#cdNonReplyAlreadyTracking').hide();
      jQuery('#cdNonReplyTrack').show();
    }
  });

}); 
