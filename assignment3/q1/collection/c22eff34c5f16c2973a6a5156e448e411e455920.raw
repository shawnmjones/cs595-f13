        <a id="bodyregion_0_interiorarticle_0_strategysections_9_ctrlScrollShare_addThis_omniEmailLink" class=" omni-link-tracker" CssClasslass="email addthis_button_email" omni-title="Social Share: Giving Pledge" omni-events="event1,event7" omni-vars="prop40,prop41,events" omni-values="prop40=email;prop41=Giving Pledge" style="text-decoration: none;">
